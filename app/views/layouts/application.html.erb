<!DOCTYPE html>
<html lang="ja">

  <head>
    <title>資格管理システム</title>
    <%= csrf_meta_tags %>
		<%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
	<% if flash[:notice] %>
	<div class="flash">
	<%= flash[:notice] %>
	</div>
	<% end %>


	<%= render 'layouts/alert' %>
	<div class="container">
		<%= render "layouts/navigation" %>
		<br></br><br></br>
		<%= yield %>
	</div>

	</body>
</html>



